{
  "name": "react-redux-webpack-base",
  "version": "0.0.1",
  "description": "Simple app with webpack 4.",
  "author": "Mihir",
  "main": "build",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "node server.js",
    "cleanup": "rm -rf build/* && rm -rf dist/* && rm -rf coverage/*",
    "heroku-postbuild": "webpack -p --env.prod",
    "build": "cross-env NODE_ENV=production webpack --env.prod",
    "tests": "cross-env BABEL_ENV=test mocha --require babel-core/register --timeout 10000 src/**/*.spec.js",
    "tests-coverage": "cross-env BABEL_ENV=test mocha --require babel-core/register --timeout 10000 coverage/**/*.spec.js",
    "pre-coverage": "cross-env istanbul instrument src --output coverage --no-compact --complete-copy --es-modules",
    "get-coverage": "npm run pre-coverage && npm run tests-coverage -- -R test/istanbul-reporter.js",
    "bump:major": "npm version major && git push --follow-tags",
    "bump:minor": "npm version minor && git push --follow-tags",
    "bump:patch": "npm version patch && git push --follow-tags",
    "precommit": "lint-staged",
    "lint": "eslint --ext .jsx --ext .js src/",
    "lint-fix": "eslint --ext .jsx --ext .js --fix src/"
  },
  "dependencies": {
    "express": "^4.16.4",
    "flexbox-react": "^4.3.3",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "^16.7.0",
    "react-google-maps": "^9.4.5",
    "react-redux": "^6.0.0",
    "react-router5": "^5.4.2",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-router5": "^5.2.1",
    "redux-thunk": "^2.2.0",
    "router5": "^5.8.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.0.1",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.5.4",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "v4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "how-the-test-was-won": "^1.0.28",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "istanbul-api": "^2.0.6",
    "istanbul-lib-coverage": "^2.0.1",
    "js-yaml": "^3.12.0",
    "lint-staged": "^8.1.0",
    "mocha": "^6.1.4",
    "node-sass": "^7.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.15.3",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1 ",
    "stylelint": "^10.0.1",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.6.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}

{
  "name": "react-redux-webpack-base",
  "version": "0.0.1",
  "description": "Simple app with webpack 4.",
  "author": "Mihir",
  "main": "build",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "cleanup": "rm -rf build/* && rm -rf dist/* && rm -rf coverage/*",
    "build": "cross-env NODE_ENV=production webpack --env.prod",
    "nsp": "nsp check --threshold=9",
    "tests": "cross-env BABEL_ENV=test mocha --require babel-core/register --timeout 10000 src/**/*.spec.js",
    "tests-coverage": "cross-env BABEL_ENV=test mocha --require babel-core/register --timeout 10000 coverage/**/*.spec.js",
    "pre-coverage": "cross-env istanbul instrument src --output coverage --no-compact --complete-copy --es-modules",
    "get-coverage": "npm run pre-coverage && npm run tests-coverage -- -R test/istanbul-reporter.js",
    "bump:major": "npm version major && git push --follow-tags",
    "bump:minor": "npm version minor && git push --follow-tags",
    "bump:patch": "npm version patch && git push --follow-tags"
  },
  "dependencies": {
    "flexbox-react": "^4.3.3",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router5": "^5.4.2",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.7.2",
    "redux-router5": "^5.2.1",
    "redux-thunk": "^2.2.0",
    "router5": "^5.8.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-rewire-exports": "^0.3.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.10",
    "dotenv-webpack": "^1.5.4",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "v4.0.0-beta.0",
    "file-loader": "1.1.10",
    "filemanager-webpack-plugin": "^1.0.24",
    "how-the-test-was-won": "^1.0.28",
    "html-webpack-plugin": "^3.0.6",
    "istanbul": "1.1.0-alpha.1",
    "istanbul-api": "^1.2.1",
    "istanbul-lib-coverage": "^1.1.1",
    "mocha": "^5.0.4",
    "node-sass": "^4.5.3",
    "nsp": "^3.1.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^2.1.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "3.0.0-beta.7",
    "redux-mock-store": "^1.2.3",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0",
    "style-loader": "^0.20.2 ",
    "stylelint": "^7.10.1",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-scss": "^1.4.4",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "3.1.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}

{
  "name": "react-redux-webpack-base",
  "version": "0.0.1",
  "description": "Simple app with webpack 4.",
  "author": "Mihir",
  "main": "build",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "audit-check": "npm-audit-ci-wrapper --ignore-dev-dependencies -t low",
    "cleanup": "rm -rf build/* && rm -rf dist/* && rm -rf coverage/*",
    "build": "cross-env NODE_ENV=production webpack --env.prod",
    "bump:major": "npm version major && git push --follow-tags",
    "bump:minor": "npm version minor && git push --follow-tags",
    "bump:patch": "npm version patch && git push --follow-tags",
    "lint": "eslint --ext .jsx --ext .js src/",
    "lint:fix": "eslint --ext .jsx --ext .js --fix src/"
  },
  "dependencies": {
    "flexbox-react": "4.4.0",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "ramda": "0.26.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.2",
    "react-router5": "7.1.0",
    "react-toolbox": "2.0.0-beta.13",
    "redux": "4.0.1",
    "redux-router5": "7.0.2",
    "redux-thunk": "2.3.0",
    "request": "2.88.0",
    "router5": "7.0.2",
    "router5-plugin-browser": "7.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-rewire": "1.2.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-preset-stage-0": "6.24.1",
    "copy-webpack-plugin": "5.0.2",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "2.7.0",
    "js-yaml": "3.13.1",
    "lint-staged": "8.2.1",
    "node-sass": "4.12.0",
    "npm-audit-ci-wrapper": "2.2.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "5.2.2",
    "prettier": "1.17.0",
    "raw-loader": "2.0.0",
    "react-hot-loader": "3.1.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "10.0.1",
    "stylelint-config-css-modules": "1.4.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-order": "2.2.1",
    "stylelint-scss": "3.6.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "husky": {
    "hooks": {
      // "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}